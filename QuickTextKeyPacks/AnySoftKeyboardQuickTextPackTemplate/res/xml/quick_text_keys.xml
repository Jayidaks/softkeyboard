<?xml version="1.0" encoding="UTF-8"?>
<QuickTextKeys>
	<!--  
	Some notes:
	1) Generate a UUID
	2) you can only use the popupkeyboard OR the list. If you want both, then use two QuickTextKey nodes.
	-->
	<QuickTextKey
        id="random UUID here"
        nameResId="@string/key_name"
		
		popupKeyboard="@xml/popup_keyboard"
		
		popupListText="@array/items_names"
		popupListOutput="@array/items_output_texts"
		
        keyLabel="@string/key_label"
        keyOutputText="@string/key_default_output"
        description="Created by YOU!"
        index="1"/>
</QuickTextKeys>