<manifest xmlns:android="http://schemas.android.com/apk/res/android" 
        package="com.menny.android.anysoftkeyboard"
        android:versionCode="16" android:versionName="20100212-rock-testers">
	<application android:label="@string/ime_name" android:icon="@drawable/icon_8_key">
	    <service android:permission="android.permission.BIND_INPUT_METHOD" android:name="SoftKeyboard">
	        <intent-filter>
	            <action android:name="android.view.InputMethod" />
	        </intent-filter>
	        <meta-data android:name="android.view.im" android:resource="@xml/method" />
	    </service>
	    <activity android:name="SoftKeyboardSettings" android:label="Settings">
	    </activity>
	    <activity android:name=".MainForm"
                  android:label="@string/ime_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <!-- 
             <intent-filter> 
                <action android:name="com.menny.android.anysoftkeyboard.KEYBOARD" />
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.ALTERNATIVE" />
                <category android:name="android.intent.category.SELECTED_ALTERNATIVE" />
            </intent-filter>
            --> 
        </activity>
        <!-- Keyboard resolver activity -->
        <activity android:name="com.menny.android.anysoftkeyboard.keyboards.KeyboardsResolverActivity"></activity>
        
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.EnglishKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.english.qwerty"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.AZERTYKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.french.AZERTY"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.BepoKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.french.bepo"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.ColemakKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.english.colemak"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.DVORAKKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.english.dvorak"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.TerminalKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.english.terminal"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.ArabicKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.arabic"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.LaoKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.lao"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.DanishKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.danish"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.NorwegianKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.norwegian"/>         
        
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.SvorakKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.svorak"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.SpanishKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.spanish"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.CatalanKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.catalan"/>
        <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.GermanKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.german"/>
          
          <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.HungarianKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.hungarian.qwertz"/>
          <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.Hungarian4RowsKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.hungarian.qwertz4row"/>
          
          <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.SwissKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.swiss.azerty"/>
          
          <provider android:name="com.menny.android.anysoftkeyboard.keyboards.providers.FrenchCanadaKeyboardProvider"
          android:authorities="com.anysoftkeyboard.keyboard.french.canada"/>
	</application>

	<uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_USER_DICTIONARY" />
    <uses-permission android:name="android.permission.WRITE_USER_DICTIONARY" />
	<uses-sdk android:minSdkVersion="3" android:targetSdkVersion="4"></uses-sdk>
	
	<supports-screens
          android:largeScreens="true" 
          android:normalScreens="true"
          android:smallScreens="true"
          android:anyDensity="false" />
          <!-- http://d.android.com/guide/practices/screens_support.html read about legacy. -->
</manifest>
